<?xml version='1.0' encoding='utf-8'?>
<Context>
    <WatchedResource>WEB-INF/web.xml</WatchedResource>
    <WatchedResource>${catalina.base}/conf/web.xml</WatchedResource>

  <Manager className="com.crimsonhexagon.rsm.redisson.SingleServerSessionManager"
	endpoint="redis.marathon.containerip.dcos.thisdcos.directory:7000,redis.marathon.containerip.dcos.thisdcos.directory:7001,redis.marathon.containerip.dcos.thisdcos.directory:7002"
	sessionKeyPrefix="_rsm_"
	saveOnChange="false"
	forceSaveAfterRequest="false"
	dirtyOnMutation="false"
	ignorePattern=".*\\.(ico|png|gif|jpg|jpeg|swf|css|js)$"
	connectionPoolSize="20"
	database="0"
	timeout="600000"
	pingTimeout="600000"
	retryAttempts="40"
	retryInterval="1000"/>
</Context>
