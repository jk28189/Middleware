<?xml version='1.0' encoding='utf-8'?>
<Context>
    <WatchedResource>WEB-INF/web.xml</WatchedResource>
    <WatchedResource>${catalina.base}/conf/web.xml</WatchedResource>

  <Manager className="com.crimsonhexagon.rsm.redisson.SingleServerSessionManager"
	endpoint="redis.marathon.autoip.dcos.thisdcos.directory:6379"
	sessionKeyPrefix="_rsm_"
	saveOnChange="false"
	forceSaveAfterRequest="false"
	dirtyOnMutation="false"
	ignorePattern=".*\\.(ico|png|gif|jpg|jpeg|swf|css|js)$"
	connectionPoolSize="10"
	database="0"
	timeout="60000"
	connectionMinimumIdleSize="1"
	pingTimeout="60000"
	retryAttempts="40"
	retryInterval="1000"/>
</Context>
